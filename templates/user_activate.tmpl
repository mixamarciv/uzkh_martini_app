	
	
	<div class="container">
		<div class="col-md-10 col-md-offset-1">
			{{if .success}}
			<div class="alert alert-dismissible alert-success">
			  <h3>{{.success}}</h3>
			</div>
			{{end}}
			{{if .error}}
			<div class="alert alert-dismissible alert-danger">
			  <h3><b>ОШИБКА:</b> {{.error}}</h3>
			</div>
			{{end}}
		</div>
	</div>
	





